<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/profile_fragment">

    <fragment
        android:id="@+id/testing"
        android:name="com.itis.devinterview.ui.fragment.TestingFragment"
        android:label="Testing"
        tools:layout="@layout/fragment_testing">
        <action
            android:id="@+id/action_testing_to_selectionLanguage"
            app:destination="@id/selectionLanguage" />
    </fragment>
    <fragment
        android:id="@+id/selectionLanguage"
        android:name="com.itis.devinterview.ui.fragment.SelectionLanguageFragment"
        android:label="SelectionLanguage"
        tools:layout="@layout/fragment_selection_language">
        <action
            android:id="@+id/action_selectionLanguage_to_ticketsFragment"
            app:destination="@id/ticketsFragment" />
    </fragment>
    <fragment
        android:id="@+id/ticketsFragment"
        android:name="com.itis.devinterview.ui.fragment.TicketsFragment"
        android:label="TicketsFragment"
        tools:layout="@layout/fragment_tickets"/>
    <fragment
        android:id="@+id/educationFragment"
        android:name="com.itis.devinterview.ui.fragment.EducationFragment"
        android:label="EducationFragment"
        tools:layout="@layout/fragment_education"/>
    <fragment
        android:id="@+id/settings_fragment"
        android:name="com.itis.devinterview.ui.fragment.SettingsFragment"
        android:label="SettingsFragment"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout="@layout/fragment_settings">
        <action
            android:id="@+id/action_settings_fragment_to_configure_notifications_fragment"
            app:destination="@id/configure_notifications_fragment" />
        <action
            android:id="@+id/action_settings_fragment_to_feedback_fragment"
            app:destination="@id/feedback_fragment" />
        <action
            android:id="@+id/action_settings_fragment_to_user_agreement_fragment"
            app:destination="@id/user_agreement_fragment" />
    </fragment>
    <fragment
        android:id="@+id/configure_notifications_fragment"
        android:name="com.itis.devinterview.ui.fragment.ConfigureNotificationsFragment"
        android:label="ConfigureNotificationsFragment"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout="@layout/fragment_configure_notifications" />
    <fragment
        android:id="@+id/feedback_fragment"
        android:name="com.itis.devinterview.ui.fragment.FeedbackFragment"
        android:label="FeedbackFragment"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout="@layout/fragment_feedback" />
    <fragment
        android:id="@+id/user_agreement_fragment"
        android:name="com.itis.devinterview.ui.fragment.UserAgreementFragment"
        android:label="UserAgreementFragment"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout="@layout/fragment_user_agreement" />
    <fragment
        android:id="@+id/secondaryExamFragment"
        android:name="com.itis.devinterview.ui.fragment.SecondaryExamFragment"
        android:label="fragment_secondary_exam"
        tools:layout="@layout/fragment_secondary_exam" />
    <fragment
        android:id="@+id/profile_fragment"
        android:name="com.itis.devinterview.ui.fragment.ProfileFragment"
        android:label="ProfileFragment"
        tools:layout="@layout/fragment_profile">
        <action
            android:id="@+id/action_profile_fragment_to_settings_fragment"
            app:destination="@id/settings_fragment" />
    </fragment>
</navigation>